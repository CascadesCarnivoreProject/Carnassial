<carnassial:LocalizedApplication x:Class="Carnassial.Editor.App"
                                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                 xmlns:carnassial="clr-namespace:Carnassial;assembly=Carnassial"
                                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                 StartupUri="EditorWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="EditorWindowStyle.xaml" />
                <ResourceDictionary Source="/Carnassial;component/Properties/SharedResources.xaml" />
                <ResourceDictionary Source="Properties/Resources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</carnassial:LocalizedApplication>
